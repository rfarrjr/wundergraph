{
  "private": true,
  "name": "@test/nextjs-integrations",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wundergraph/wundergraph.git"
  },
  "homepage": "https://wundergraph.com",
  "author": {
    "name": "WunderGraph Maintainers",
    "email": "info@wundergraph.com"
  },
  "bugs": {
    "url": "https://github.com/wundergraph/wundergraph/issues"
  },
  "scripts": {
    "clean": "rimraf ./.next",
    "dev": "concurrently \"npm run nextDev\" \"npm run wundergraph\"",
    "wundergraph": "wunderctl up",
    "generate": "wunderctl generate",
    "browser": "wait-on \"http-get://localhost:3003\" && wait-on \"http-get://localhost:9991\" && open-cli http://localhost:3003",
    "build": "pnpm generate && next build && pnpm check",
    "check": "tsc --noEmit",
    "nextDev": "next dev --port 3003",
    "start": "next start --port 3003"
  },
  "dependencies": {
    "@wundergraph/nextjs": "workspace:*",
    "@wundergraph/sdk": "workspace:*",
    "@wundergraph/swr": "workspace:*",
    "next": "^14.2.4",
    "react": "18.2.0",
    "react-dom": "^18.3.1",
    "stripe": "^11.18.0",
    "swr": "^2.2.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^18.19.34",
    "@types/react": "^18.3.3",
    "concurrently": "^6.5.1",
    "graphql": "^16.8.1",
    "open-cli": "^7.2.0",
    "typescript": "^5.4.5",
    "wait-on": "^7.2.0"
  },
  "sideEffects": false
}
