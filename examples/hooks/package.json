    "check": "tsc --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@wundergraph/sdk": "workspace:*",
    "graphql": "^16.3.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/node": "^17.0.15",
    "@types/node-fetch": "^2.6.2",
    "jest": "^29.6.1",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.8.0",
    "typescript": "^5.4.5",
    "wait-on": "^7.2.0"
  }
}
