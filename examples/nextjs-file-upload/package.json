{
  "name": "wundergraph-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run start:services && run-p dev wundergraph open",
    "start:services": "cd minio && ./setup.sh",
    "open": "wait-on -d 500 http://localhost:9991 && open-cli http://localhost:3000",
    "wundergraph": "wunderctl up",
    "build:wundergraph": "wunderctl generate",
    "build:next": "next build",
    "build": "npm run build:wundergraph && npm run build:next",
    "dev": "next dev",
    "test:playwright": "npx -- playwright test",
    "check": "tsc --noEmit",
    "cleanup": "cd minio && docker-compose down -v"
  },
  "dependencies": {
    "@wundergraph/nextjs": "workspace:*",
    "@wundergraph/sdk": "workspace:*",
    "graphql": "^16.3.0",
    "next": "^14.0.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "swr": "^2.2.0",
    "zod": "^3.22.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.30.0",
    "@types/node": "^17.0.15",
    "@types/react": "^18.3.3",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "ts-node": "^10.8.0",
    "typescript": "^5.4.5",
    "wait-on": "^7.2.0"
  }
}
