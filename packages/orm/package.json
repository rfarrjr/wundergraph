{
  "name": "@wundergraph/orm",
  "version": "0.3.1",
  "license": "Apache-2.0",
  "description": "WunderGraph ORM",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    }
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "type-test": "tsd"
  },
  "files": [
    "dist"
  ],
  "tsd": {
    "directory": "./test-d",
    "compilerOptions": {
      "typeRoots": []
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "dist"
    ],
    "testEnvironment": "node"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wundergraph/wundergraph.git"
  },
  "dependencies": {
    "@graphql-tools/utils": "^9.2.1",
    "@timkendall/tql": "1.0.0-rc.8",
    "graphql": "^16.8.1",
    "hotscript": "^1.0.13",
    "ix": "^5.0.0",
    "outvariant": "^1.4.2",
    "remeda": "^1.61.0",
    "ts-poet": "^6.9.0",
    "type-fest": "^3.13.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/node": "^18.19.34",
    "graphql-request": "^5.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.4",
    "ts-toolbelt": "^9.6.0",
    "tsd": "^0.31.0",
    "typescript": "^5.4.5"
  },
  "homepage": "https://wundergraph.com",
  "author": {
    "name": "WunderGraph Maintainers",
    "email": "info@wundergraph.com"
  },
  "keywords": [
    "wundergraph",
    "orm",
    "graphql"
  ],
  "sideEffects": false
}
